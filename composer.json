{
    "name"        : "frostbane/php-router",
    "description" : "Simple PHP Router, supports REST and reverse routing.",
    "keywords"    : [
        "router",
        "routing",
        "rest"
    ],
    "homepage"    : "https://github.com/frostbane/php-router",
    "license"     : "MIT",
    "authors"     : [
        {
            "name"    : "Danny van Kooten",
            "email"   : "dannyvankooten@gmail.com",
            "homepage": "http://dannyvankooten.com/"
        },
        {
            "name" : "Jefersson Nathan",
            "email": "malukenho@phpse.net"
        }
    ],
    "require"     : {
        "php"                  : "^5.3.0|^7.0",
        "fig/http-message-util": "^1.1"
    },
    "require-dev" : {
        "malukenho/docheader"      : "^0.1.5",
        "phpunit/phpunit"          : "4.8.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload"    : {
        "psr-4": {
            "PHPRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPRouter\\Test\\"          : "tests/src/PHPRouterTest/",
            "PHPRouter\\Test\\Fixtures\\": "tests/Fixtures/"
        }
    }
}
